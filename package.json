{
  "name": "buildo-react-components",
  "version": "0.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "rm -rf lib && mkdir lib && node-sass src --importer sass-importer.js --include-path node_modules -o lib && babel --loose --stage 0 --out-dir lib src",
    "lint": "eslint src",
    "preversion": "npm run lint && npm run test && npm run build-examples",
    "prepublish": "npm run build",
    "build-kitchen-sink": "npm run clean && webpack --config kitchen-sink/webpack.config.build.js --progress",
    "kitchen-sink": "npm run clean && webpack-dev-server --config kitchen-sink/webpack.config.js --progress --hot --inline",
    "build-examples": "npm run clean && webpack --config examples/webpack.config.build.js --progress",
    "sass-examples": "node-sass src --importer sass-importer.js --include-path node_modules -o examples",
    "start": "npm run clean && npm run sass-examples && webpack-dev-server --config examples/webpack.config.js --progress --hot --inline",
    "clean": "rm -f examples/bundle.js examples/bundle.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:buildo/react-components"
  },
  "keywords": [],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buildo/react-components/issues"
  },
  "homepage": "https://github.com/buildo/react-components",
  "dependencies": {
    "classnames": "^2.2.1",
    "lodash": "^3.10.1",
    "mobile-detect": "^1.2.1",
    "react-select": "^0.6.1",
    "sass-flex-mixins": "^0.1.0"
  },
  "devDependencies": {
    "axios": "^0.8.1",
    "babel": "^5.8.12",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "component-playground": "^0.1.2",
    "compression-webpack-plugin": "^0.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "expect": "^1.13.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-webpack-plugin": "^1.7.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.16",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.4.2",
    "normalize-css": "^2.3.1",
    "object-assign": "^4.0.1",
    "raw-loader": "^0.5.1",
    "rc-datepicker": "buildo/rc-datepicker",
    "react": "^0.13.3 <0.14.0",
    "react-autosize-textarea": "buildo/react-autosize-textarea",
    "react-cookie-banner": "buildo/react-cookie-banner",
    "react-input-link": "buildo/react-input-link",
    "react-router-transition-context": "^0.13.3",
    "react-sidebar": "^1.1.0",
    "revenge": "^0.3.4",
    "revenge-react-components": "buildo/revenge-react-components",
    "require-dir": "^0.3.0",
