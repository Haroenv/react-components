{
  "name": "buildo-react-components",
  "version": "0.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "rm -rf lib && mkdir lib && node-sass src --importer sass-importer.js --include-path node_modules -o lib && babel --loose --stage 0 --out-dir lib src",
    "lint": "eslint src",
    "preversion": "npm run lint && npm run test && npm run build-examples",
    "prepublish": "npm run build",
    "build-kitchen-sink": "npm run clean && webpack --config kitchen-sink/webpack.config.build.js --progress",
    "kitchen-sink": "npm run clean && webpack-dev-server --config kitchen-sink/webpack.config.js --progress --hot --inline",
    "build-examples": "npm run clean && webpack --config examples/webpack.config.build.js --progress",
    "start": "npm run clean && node-sass src --importer sass-importer.js --include-path node_modules -o examples && webpack-dev-server --config examples/webpack.config.js --progress --hot --inline",
    "clean": "rm -f examples/bundle.js examples/bundle.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:buildo/react-components"
  },
  "keywords": [],
  "author": "Giovanni Gonzaga <giovanni@buildo.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buildo/react-components/issues"
  },
  "homepage": "https://github.com/buildo/react-components",
  "dependencies": {
    "classnames": "^2.1.3",
    "lodash": "^3.10.1",
    "mobile-detect": "^1.2.1",
    "rc-datepicker": "^2.0.0",
    "react-autosize-textarea": "^0.2.4",
    "react-cookie-banner": "^0.0.9",
    "react-input-link": "^0.0.3",
    "react-select": "^0.6.1",
    "sass-flex-mixins": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.8.25",
    "babel-eslint": "^3.1.11",
    "babel-loader": "^5.1.3",
    "component-playground": "^0.1.2",
    "eslint": "^0.23.0",
    "eslint-loader": "^0.14.0",
    "eslint-plugin-react": "^2.5.2",
    "expect": "^1.6.0",
    "file-loader": "^0.8.5",
    "isparta-loader": "^0.2.0",
    "karma": "^0.13.0",
    "karma-bamboo-reporter": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.3.3",
    "object-assign": "^4.0.1",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3 <0.14.0",
    "require-dir": "^0.3.0",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  },
  "peerDependencies": {
    "react": "^0.13.3 <0.14.0"
  }
}
